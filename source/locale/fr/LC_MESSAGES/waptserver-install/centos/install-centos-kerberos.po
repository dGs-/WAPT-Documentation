msgid "Configuring Kerberos authentication on CentOS/ RedHat"
msgstr "Configurer l'authentification Kerberos avec Debian ou CentOS / RedHat"

#: ../../source/wapt_server_install/centos/install_centos_kerberos.rst:21
msgid "Kerberos, authentication, Debian, WAPT, documentation, RedHat, CentOS"
msgstr "Kerberos, authentification, Debian, documentation, CentOS"

